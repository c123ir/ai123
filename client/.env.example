# متغیرهای محیطی برای کلاینت
# کپی این فایل را با نام .env ذخیره کنید

# آدرس API سرور
REACT_APP_API_URL=http://localhost:5454/api/v1

# پورت اجرای کلاینت
PORT=3434

# نسخه برنامه
REACT_APP_VERSION=0.1.0

# محیط اجرا (development, production, test)
NODE_ENV=development

# تنظیمات اختیاری
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_DEFAULT_LANGUAGE=fa

# فعال/غیرفعال کردن ابزارهای توسعه Redux
REACT_APP_REDUX_DEVTOOLS=true

# کلید API برای نقشه (در صورت نیاز)
# REACT_APP_MAP_API_KEY=your_map_api_key

# تنظیمات Firebase (در صورت نیاز)
# REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
# REACT_APP_FIREBASE_AUTH_DOMAIN=your_auth_domain
# REACT_APP_FIREBASE_PROJECT_ID=your_project_id
# REACT_APP_FIREBASE_STORAGE_BUCKET=your_storage_bucket
# REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
# REACT_APP_FIREBASE_APP_ID=your_app_id
